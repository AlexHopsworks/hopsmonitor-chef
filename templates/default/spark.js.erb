function makeYarnAppInfo(){
    if(_.isUndefined(ARGS.app)) {
	throw new Error("app URL parameter required");
    }
    var date = new Date();
    date.setDate(date.getDate() - 3);
    var from = date;
    if(!_.isUndefined(ARGS.from)) {
	from = new Date(ARGS.from);
    }
    var now = true;
    var refresh = '5s';
    var to;
    if(!_.isUndefined(ARGS.to)) {
        to = new Date(ARGS.to);
	now = false;
	refresh='';
    } else {
	to = 'now';
    }
       var app = {
	   prefix:ARGS.app,
	   now: now,
	   from: from,
	   to: to,
	   refresh:refresh
    };
    return app;
}

